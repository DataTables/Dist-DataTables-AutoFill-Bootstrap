<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>2.3.8</version>
        <id>datatables.net-autofill-bs</id>
        <description>This is AutoFill for DataTables with styling for [Bootstrap](https://getbootstrap.com/docs/3.3/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-AutoFill-Bootstrap.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>AutoFill  Excel  Bootstrap  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-autofill" version=">=2.3.7"/>
            <dependency id="datatables.net-bs" version=">=1.10.25"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/autoFill.bootstrap.js" target="content\Scripts"/>
        <file src="js/autoFill.bootstrap.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/autoFill.bootstrap.min.js" target="content\Scripts"/>
        <file src="js/autoFill.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/autoFill.bootstrap.css" target="content\Content"/>
        <file src="css/autoFill.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/autoFill.bootstrap.min.css" target="content\Content"/>
        <file src="css/autoFill.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/autoFill.bootstrap.d.ts" target="content\Scripts"/>
        <file src="types/autoFill.bootstrap.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

