<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>datatables.net-autofill-bs</id>
        <version>2.3.6</version>
        <description>AutoFill for DataTables</description>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>autofill DataTables jQuery table Bootstrap</tags>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-AutoFill-Bootstrap.git"/>
        <dependencies>
            <dependency id="jquery" version="1.7.0"/>
            <dependency id="datatables.net" version="1.10.15"/>
            <dependency id="datatables.net-autofill" version="2.3.5"/>
            <dependency id="bootstrap" version="3.*"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content" />
        </contentFiles>
    </metadata>
    <files>
        <file src="js/autofill.bootstrap.js" target="content\Script"/>
        <file src="js/autofill.bootstrap.min.js" target="content\Script"/>
        <file src="css/autofill.bootstrap.css" target="content\Content"/>
        <file src="css/autofill.bootstrap.min.css" target="content\Content"/>

        <file src="js/autofill.bootstrap.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/autofill.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/autofill.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/autofill.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>
